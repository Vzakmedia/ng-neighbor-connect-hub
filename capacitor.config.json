{
  "appId": "app.lovable.1a2a500fc780403294cae7185a964f07",
  "appName": "Neighborlink",
  "webDir": "dist",
  "android": {
    "allowMixedContent": true
  },
  "ios": {
    "contentInset": "automatic"
  },
  "plugins": {
    "Geolocation": {
      "permissions": ["location"]
    },
    "Camera": {
      "permissions": ["camera", "photos"]
    },
    "StatusBar": {
      "style": "dark",
      "backgroundColor": "#667eea"
    }
  }
}